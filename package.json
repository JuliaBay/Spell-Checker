{
  "name": "spellcheck",
  "displayName": "Spelling Checker (RU,EN)",
  "description": "",
  "icon":"images/icon1.png",
  "version": "0.1.0",
  "publisher": "JuliaBay",
  "engines": {
    "vscode": "^1.0.0"
  },
  "categories": [
    "Other","Linters"
